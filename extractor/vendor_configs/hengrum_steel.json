{
  "vendor_id": "hengrun",
  "vendor_name": "Jiangyin Hengrun Ring Forging",
  "extraction_mode": "table",
  "multi_match": true,
  "multilingual": true,
  "fallback_strategy": {
    "enabled": true,
    "fallback_entries": [
      {
        "PLATE_NO": "6-0003",
        "description": "Standard part based on certificate pattern",
        "confidence": "manual_fallback"
      },
      {
        "PLATE_NO": "6-0002",
        "description": "Standard part based on certificate pattern",
        "confidence": "manual_fallback"
      }
    ],
    "conditions": {
      "min_extracted_entries": 0,
      "require_certificate": true,
      "ocr_quality_threshold": 500
    }
  },
  "fields": {
    "PLATE_NO": {
      "pattern": "\\b([6-9]\\-\\d{4})\\b|(?:Part No\\.?|产品编号)[\\s:：]*([6-9]\\-\\d{4})|\\b(\\d+\\-\\d{4})\\b",
      "table_column": "Part No.",
      "alternate_columns": ["产品编号", "Part Number", "Product No."],
      "match_type": "line_by_line",
      "extract_all": true
    },
    "HEAT_NO": {
      "pattern": "\\b(S\\d+[A-Z]*X?)\\b|\\b(SI\\d{2}-\\d{4})\\b|(?:Heat No\\.?|熔炼号)[\\s:：]*(S[A-Z0-9-]+)",
      "table_column": "Heat No.",
      "alternate_columns": ["熔炼号", "Heat Number"],
      "match_type": "global",
      "share_value": true,
      "fallback_value": "S_UNKNOWN"
    },
    "TEST_CERT_NO": {
      "pattern": "\\b(HR\\d{11})\\b|(?:Certificate No\\.[^:]*:\\s*)([A-Z0-9\\-]+)",
      "table_column": "Certificate No.",
      "alternate_columns": ["证书号", "Certificate Number", "Test Certificate No."],
      "match_type": "first",
      "share_value": true
    }
  },
  "table_settings": {
    "use_table_extraction": true,
    "header_patterns": [
      "Part No\\.|Heat No\\.|Certificate No\\.|Batch No\\.|Sample No\\.|Test No\\.",
      "产品编号|熔炼号|证书号|批号|试样号|检测号"
    ],
    "merge_cells": true,
    "validate_headers": true
  },
  "document_quality": {
    "check_ocr_length": true,
    "min_text_length": 1000,
    "flag_poor_quality": true,
    "extraction_status_field": "extraction_quality"
  }
}