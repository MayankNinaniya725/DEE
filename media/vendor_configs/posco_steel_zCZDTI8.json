{
  "vendor_id": "posco",
  "vendor_name": "posco_steel",
  "extraction_mode": "table",
  "multi_match": true,
  "fields": {
    "PLATE_NO": {
      "pattern": "\\b(PP\\d{3}[A-Z]\\d{4}(?:-[A-Z]\\d{4})?)\\b|\\b(PP\\d{6}[A-Z]=\\d{3})\\b|\\b(PP\\d{6}H=\\d{3})\\b",
      "table_column": "Product No.",
      "match_type": "line_by_line",
      "extract_all": true,
      "required": true
    },
    "HEAT_NO": {
      "pattern": "\\b(SU\\d{5})\\b|\\b(SU3[0-9][6-9][0-9]{2})\\b",
      "table_column": "Heat No.",
      "match_type": "line_by_line",
      "extract_all": true,
      "required": true,
      "share_value": true,
      "ocr_alternatives": {
        "6": "8",
        "0": "8"
      }
    },
    "TEST_CERT_NO": {
      "pattern": "Certificate\\s+No\\.\\s*[:]*\\s*(\\d{6}-FP\\d{2}[A-Z]{2}-\\d{4}[A-Z]\\d-\\d{4})",
      "prefix": "Certificate No\\.",
      "match_type": "first",
      "search_header": true,
      "required": true,
      "share_value": true
    }
  },
  "table_settings": {
    "use_table_extraction": true,
    "header_patterns": [
      "Size\\s*\\|\\s*Product\\s+No\\.\\s*\\|\\s*Quantity\\s*\\|\\s*Weight\\s*\\(kg\\)\\s*\\|\\s*Heat\\s+No\\.",
      "Size\\s+Product\\s+No\\.\\s+Heat\\s+No\\."
    ],
    "merge_cells": true,
    "validate_headers": true,
    "min_required_columns": 3,
    "ignore_empty_rows": true
  },
  "metadata": {
    "document_type": "Mill Test Certificate",
    "document_identifier": "MTC",
    "issuer": "POSCO INTERNATIONAL CORPORATION"
  },
  "validation_rules": {
    "plate_no_format": "^PP\\d{3}[A-Z]\\d{4}(?:-[A-Z]\\d{4})?$",
    "heat_no_format": "^SU\\d{5}$",
    "cert_no_format": "^\\d{6}-FP\\d{2}[A-Z]{2}-\\d{4}[A-Z]\\d-\\d{4}$"
  }
}
